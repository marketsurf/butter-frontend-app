{
  "name": "butter-frontend-app",
  "description": "A frontend app for buTTer",
  "version": "0.1.0",
  "private": true,
  "author": "Marketsurf",
  "license": "MIT",
  "keywords": [
    "buTTer",
    "frontend",
    "app"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@mantine/core": "4.2.5",
    "@mantine/dropzone": "4.2.5",
    "@mantine/form": "4.2.5",
    "@mantine/hooks": "4.2.5",
    "@mantine/next": "4.2.5",
    "@mantine/prism": "4.2.5",
    "axios": "0.27.2",
    "file-saver": "2.0.5",
    "joi": "17.6.0",
    "next": "12.1.6",
    "react": "^17.0.2",
    "react-beautiful-dnd": "13.1.0",
    "react-dom": "^17.0.2",
    "tabler-icons-react": "1.48.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.3.0",
    "@commitlint/config-conventional": "16.2.4",
    "@types/file-saver": "2.0.5",
    "@types/multer": "1.4.7",
    "@types/node": "17.0.33",
    "@types/react": "18.0.15",
    "@types/react-beautiful-dnd": "13.1.2",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.23.0",
    "@typescript-eslint/parser": "5.23.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.15.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "8.0.1",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "typescript": "4.6.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
